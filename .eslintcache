nts/HomeHeader/index.js",["283"],"import React from 'react';\nimport { Link } from \"react-router-dom\";\nimport \"./style.css\"\n\nconst HomeHeader = () => {\n    return (\n        <div className=\"homeHeader\">\n            <header className=\"homeHeader__wrapper\">\n                <a className=\"homeHeader__city\">北京</a>\n                <Link to=\"/search\" className=\"homeHeader__search\">\n                    输入商户名、地点\n                </Link>\n\n                <Link to=\"/user\" className=\"homeHeader__self\">\n                    <div className=\"homeHeader__portrait\"/>\n                </Link>\n            </header>\n        </div>\n    )\n}\n\nexport default HomeHeader;","/Users/liming/Self/Project/self/front-end/final-shizhan/src/containers/ProductDetail/index.js",[],["284","285"],"/Users/liming/Self/Project/self/front-end/final-shizhan/src/components/Header/index.js",[],"/Users/liming/Self/Project/self/front-end/final-shizhan/src/containers/ProductDetail/components/ShopInfo/index.js",[],"/Users/liming/Self/Project/self/front-end/final-shizhan/src/containers/ProductDetail/components/BuyButton/index.js",[],"/Users/liming/Self/Project/self/front-end/final-shizhan/src/containers/ProductDetail/components/Detail/index.js",[],"/Users/liming/Self/Project/self/front-end/final-shizhan/src/containers/ProductDetail/components/ProductOverview/index.js",["286"],"import React, { Component } from \"react\";\nimport \"./style.css\";\n\nclass ProductOverview extends Component {\n    render() {\n        const { picture } = this.props.data\n        return (\n            <div className=\"productOverview\">\n                <div className=\"productOverview__header\">\n                    <div className=\"productOverview__imgContainer\">\n                        <img\n                            alt=\"\"\n                            className=\"productOverview__img\"\n                            src={picture}\n                        />\n                    </div>\n                    <div className=\"productOverview__baseInfo\">\n                        <div className=\"productOverview__title\">院落创意菜</div>\n                        <div className=\"productOverview__content\">\n                            仅售19.9元！价值48元的百香果（冷饮）1扎，提供免费WiFi。\n                        </div>\n                    </div>\n                </div>\n                <div className=\"productOverview__purchase\">\n                    <span className=\"productOverview__symbol\">¥</span>\n                    <span className=\"productOverview__price\">19.9</span>\n                    <span className=\"productOverview__price--old\">¥48</span>\n                    <a className=\"productOverview__btn\">立即购买</a>\n                </div>\n                <ul className=\"productOverview__remark\">\n                    <li className=\"productOverview__remarkItem\">\n                        <i className=\"productOverview__sign1\" />\n                        <span className=\"productOverview__desc\">随时可退</span>\n                    </li>\n                    <li className=\"productOverview__remarkItem\">\n                        <i className=\"productOverview__sign2\" />\n                        <span className=\"productOverview__desc\">过期自动退</span>\n                    </li>\n                </ul>\n            </div>\n        );\n    }\n}\n\nexport default ProductOverview;","/Users/liming/Self/Project/self/front-end/final-shizhan/src/redux/store.js",[],"/Users/liming/Self/Project/self/front-end/final-shizhan/src/utils/createReducer.js",[],"/Users/liming/Self/Project/self/front-end/final-shizhan/src/containers/Search/components/SearchHistory/index.js",[],"/Users/liming/Self/Project/self/front-end/final-shizhan/src/redux/modules/search.js",[],"/Users/liming/Self/Project/self/front-end/final-shizhan/src/containers/Search/components/SearchBox/index.js",[],"/Users/liming/Self/Project/self/front-end/final-shizhan/src/containers/Search/index.js",[],"/Users/liming/Self/Project/self/fron